{
  "name": "babel-express-sample",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && node dist/bin/www",
    "lint": "eslint app.js src",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./babelrc,./package.json,./npm-debug.log --copy-files",
    "dev": "nodemon --exec babel-node bin/www --watch app.js src views"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.18.2",
    "express-jwt": "^6.0.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "pug": "^3.0.2",
    "ramda": "^0.27.1",
    "sequelize": "^6.6.5",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/eslint-parser": "^7.14.9",
    "@babel/node": "^7.14.9",
    "@babel/preset-env": "^7.14.9",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.15",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "2500"
  }
}
