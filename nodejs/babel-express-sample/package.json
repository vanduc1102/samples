{
  "name": "babel-express-sample",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && node dist/bin/www",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./babelrc,./package.json,./npm-debug.log --copy-files",
    "dev": "nodemon --exec babel-node bin/www --watch app.js src views"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "2500"
  }
}
